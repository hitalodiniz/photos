{
  "name": "photos",
  "version": "0.1.162",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "node scripts/generate-version-env.js && next dev",
    "build": "node scripts/generate-version-env.js && next build",
    "start": "next start",
    "test:ui": "vitest --ui",
    "lint": "eslint .",
    "test": "set NODE_OPTIONS=--max-old-space-size=4096 && vitest",
    "test:clean": "vitest --clearCache && npx vitest src/contexts/AuthContext.spec.tsx",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=8192' vitest run --coverage",
    "test:critical": "vitest run src/core/services/auth.service.spec.ts src/hooks/useSupabaseSession.spec.ts src/components/providers/AuthContext.spec.tsx src/core/services/google.service.spec.ts src/core/logic/auth-gallery.spec.ts src/lib/supabase.client.spec.ts src/lib/supabase.server.spec.ts src/middleware.spec.ts src/app/api/auth/callback/route.spec.ts src/app/api/auth/google/route.spec.ts src/actions/auth.actions.spec.ts \"src/features/galeria/components/admin/GaleriaFormContent.spec.tsx\"",
    "validate:critical": "bash scripts/validate-critical-files.sh",
    "protect:check": "node scripts/protect-critical-files.js --check-only",
    "protect:list": "node scripts/protect-critical-files.js --list",
    "check:protection": "node scripts/protect-critical-files.js --check-only",
    "list:critical": "node scripts/protect-critical-files.js --list",
    "setup:protection": "chmod +x scripts/protect-critical-files.js && chmod +x scripts/validate-critical-files.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@supabase/auth-js": "^2.87.1",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.86.2",
    "@tailwindcss/typography": "^0.5.19",
    "@vercel/analytics": "^1.6.1",
    "cookies-next": "^6.1.1",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.26",
    "googleapis": "^129.0.0",
    "jose": "^6.1.3",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "jszip": "^3.10.1",
    "lucide-react": "^0.561.0",
    "next": "^15.5.9",
    "nodemailer": "^7.0.12",
    "photoswipe": "^5.4.4",
    "postcss-import": "^16.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
    "react-masonry-css": "^1.0.16",
    "react-photoswipe-gallery": "^4.0.0",
    "react-quick-pinch-zoom": "^5.1.1",
    "slugify": "^1.6.6",
    "stripe": "^20.1.0",
    "swiper": "^12.1.0",
    "ua-parser-js": "^2.0.9",
    "web-push": "^3.6.7",
    "xlsx": "^0.18.5",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.22",
    "eslint": "^9",
    "eslint-config-next": "16.0.6",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "supabase": "^2.76.12",
    "tailwindcss": "^3.4.18",
    "typescript": "^5",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  }
}
